<div id="landing">
  <% if user_signed_in? %>
    <%= link_to 'Logout', destroy_user_session_path, method: 'delete', class: 'btn btn-default pull-right' %>
  <% else %>
    <%= link_to 'Login', new_user_session_path, class: 'btn btn-default pull-right' %>
  <% end %>
  <div class="logo"></div>
  <div class="text">
    <h1>Connecting & Encouraging</h1>
    <h2>Central Ohio Revivalists</h2>
    <br><br>
    <%= link_to 'Create a profile', new_user_registration_path, 
    class: 'btn btn-primary btn-lg' %>
  </div>
</div>